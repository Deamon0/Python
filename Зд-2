a = []
for b in range(1000):
    if b % 2 != 0:
     a.append(b ** 3);
print(a);

m = []
h = 0;
for i in a:
    b = i;
    while i != 0:
        h += i % 10;
        i = i // 10;
        if h % 7 == 0:
            m.append(b);
print(sum(m));

summ = 0;
for d in a:
    summ1 = 0;
    b = d;
    d += 17;
    while d != 0:
        summ1 += d % 10;
        d = d // 10;
        if summ1 % 7 == 0:
            summ += b;
print(summ);